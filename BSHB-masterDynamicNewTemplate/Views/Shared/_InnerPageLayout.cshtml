                <ul class="nav navbar-nav">
                   @for (int cnt=0;cnt<=loop;cnt++)
                    {                        
                        @foreach (var item in Model.mnus.Where(s => s.ParentMenuId == cnt).GroupBy(Obj => new { Obj.Id }).Distinct().ToList())
                        {
                            var data = Model.mnus.Where(s => s.ParentMenuId == @cnt).ToList();
                            if (data.Count == 0)
                            {
                                <li><a href="@item.FirstOrDefault().Action">@item.FirstOrDefault().Title</a></li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href=/@item.FirstOrDefault().Title/Index>@item.FirstOrDefault().Title
                                        @foreach (var submenuitem in Model.mnus.Where(s => s.ParentMenuId == @cnt).GroupBy(Obj => new { @cnt }).Distinct().ToList())
                                        {
                                            var submenudata = Model.mnus.Where(s => s.ParentMenuId == @cnt).ToList();
                                            @if (submenudata.Count > 1 && firstsubmnu>0)
                                            {
                                                <span class="caret"></span>
                                            }
                                            break;
                                        }
                                    </a>
                                    @foreach (var submenuitem in Model.mnus.Where(s => s.ParentMenuId == @cnt).GroupBy(Obj => new { @cnt }).Distinct().ToList())
                                    {
                                        var submenudata = Model.mnus.Where(s => s.ParentMenuId == @cnt).ToList();
                                        @if (submenudata.Count > 1)
                                        {
                                            <ul class="level2 dropdown-menu">
                                                @foreach (var firstItem in (Model.mnus.Where(s => s.ParentMenuId == item.FirstOrDefault().ParentMenuId).ToList()))
                                                {
                                                    @if (@firstItem.Title != @item.FirstOrDefault().Title)
                                                    {
                                                        <li class="nav-item dropdown"><a class="" href="/@firstItem.Controller/@firstItem.Action">@firstItem.Title</a></li>
                                                    }
                                                    firstsubmnu=firstsubmnu + 1;
                                                }
                                            </ul>
                                        }
                                        break;
                                    }                      
                                                                
                                </li>

                            }
                            break;
                        }                        
                   }                    
                </ul>
            </div>
           </section> 
